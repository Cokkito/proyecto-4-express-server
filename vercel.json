{
	"version": 2,
	"builds": [
		{
			"src": "server.js",
			"use": "@vercel/node"
		}
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [{ "key": "Access-Control-Allow-Origin", "value": "*" }]
		},
		{
			"source": "/https://proyecto-4-mora.vercel.app/(.*)",
			"headers": [{ "key": "Access-Control-Allow-Origin", "value": "*" }]
		}
	]
}
